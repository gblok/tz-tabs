  "babel": {
    "compact": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "scripts": {
    "prod": "webpack-cli --config .etc/webpack/prod.babel.js",
    "dev": "webpack-dev-server --config .etc/webpack/dev.babel.js",
    "dll": "webpack-cli --config .etc/webpack/dll.babel.js",
    "svgo": "svgo -f static/svg --config=.etc/svgo.yml"
